package springMavenPluto.mapper;

import java.util.List;

import springMavenPluto.model.MobileBookModel;

public interface mobileBookApi {
	public List<MobileBookModel> getMobileByUserId(Integer userId);
	public boolean addMobileByUserId(MobileBookModel mb);
	public boolean updateMobileByUserId(MobileBookModel mb);
	public boolean removeMobileByUserId(Integer userId);
}
